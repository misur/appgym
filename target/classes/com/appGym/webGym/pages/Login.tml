<html t:title="Login" t:type="layout" t:pageTitle="Login"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">

	<section id="content">
		<div class="ic">More Website Templates @ TemplateMonster.com. April
			02,
			2012!</div>
		<div class="container_12">
			<div class="grid_12">
				<div class="box-shadow">
					<div class="wrap block-2">
						<div class="col-4">
							<form t:type="form" t:id="loginForm">
								<fieldset>
									<table cellpadding="10" border="2">
										<tr>
											<td><p>Username:</p><br></br></td>
											<td><input t:type="textfield" t:id="username" t:validate="username" /></td>
										</tr>
										<tr>
											<td><p>Password:<br></br></p></td>
											<td><input type="password" t:type="passwordfield" t:id="password"
											t:validate="password" class="logininput" /></td>
										</tr>
										
										

										
										
									</table>
									<div class="btns">
										<t:linkSubmit t:value="Login" type="submit"
											class="button2">
											Login
										</t:linkSubmit>
									</div>

								</fieldset>
							</form>

						</div>
					</div>
				</div>
			</div>
			<div class="clear"></div>
		</div>
	</section>




</html>
