                              <label><textarea onBlur="if(this.value==''){this.value='Message'}" onFocus="if(this.value=='Message'){this.value=''}">Message</textarea></label>
                              <div class="btns"><a href="#" class="button2">Clear</a><a href="#" class="button2" onClick="document.getElementById('form').submit()">Send</a></div>
